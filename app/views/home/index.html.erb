  
<% if current_user %>
  Welcome <%= current_user.name %>!
  <%= link_to "Sign Out", signout_path %>
<% else %>
  <%= link_to "Sign in with Instagram", "/auth/instagram" %>
<% end %>

  <% @instagram.each do |instagram| %>

    <%= image_tag instagram.images.standard_resolution.url %>

    <% end %>
    
    
<ul>
  <% @tags.each do |t| %>
    <div>
   	<% unless t.caption.blank? %>
  		<%= t.caption.text %>
	<% end %>
    <%= image_tag t.images.standard_resolution.url %>
    </div>
  <% end %>
</ul>

    